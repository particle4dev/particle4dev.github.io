{
    "version": 2,
    "name": "www.particle4dev.com",
    "alias": "www.particle4dev.com",
    "builds": [{
        "src": "package.json",
        "use": "@now/static-build",
        "config": {
            "distDir": "public"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "$1"
        }
    ]
}
